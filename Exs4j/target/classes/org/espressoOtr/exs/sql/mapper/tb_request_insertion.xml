<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="tb_request_insertion">
  
  <insert id="insertRequestRecord" parameterType="org.espressoOtr.exs.sql.param.RequestRecord">
    INSERT INTO tb_request(masterKeyword, origin, reqDate) VALUES (#{masterKeyword},#{origin},#{reqDate})
  </insert>
  
</mapper>