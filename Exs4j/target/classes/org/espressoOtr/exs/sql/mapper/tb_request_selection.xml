<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="tb_request_selection">
  
  <select id="selectRequestRecord" resultType="org.espressoOtr.exs.sql.param.RequestRecord">
    SELECT requestCode, masterKeyword, origin, reqDate FROM tb_request WHERE requesetCode =#{requestCode}
  </select>
  
</mapper>